+            int high = in.read();
+            int low = in.read();
+            if ((high | low) == -1) {
+                throw new EOFException("Zstd dictionary version could not be read from chunk.");
+            }
+            short version = (short) ((high << 8) | low);
+            com.github.luben.zstd.ZstdInputStream retVal = new com.github.luben.zstd.ZstdInputStream(in);
