+            out.write((CURRENT_ZSTD_VERSION >> 8) & 0xFF);
+            out.write(CURRENT_ZSTD_VERSION & 0xFF);
+            com.github.luben.zstd.ZstdOutputStream retVal = new com.github.luben.zstd.ZstdOutputStream(out);
